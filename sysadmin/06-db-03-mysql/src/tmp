sudo docker run -dp 3306:3306 --name test_mysql -v /mysql-db/:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=example test_db
sudo docker exec -i test_mysql sh -c 'exec mysql -uroot -pexample test_db' < /home/lokli/devops-netology/sysadmin/06-db-03-mysql/test-data/test_dump.sql





sudo docker exec -it mysql sh -c 'mysql -uroot -pexample -c ';


sudo docker exec -i mysql sh -c 'exec mysql -uroot -pexample mysql' < ../test-data/test_dump.sql




